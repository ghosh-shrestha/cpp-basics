#include<iostream>
#include<algorithm>
#include<vector>
usng namespace std;
int main(){
  vector<int>v1;
  int n,p;
  cout<<"enter size: ";
  cin>>n;
  for(int i=0;i<n;i++){
    cout<<"v1["<<i<<"] ";
    cin>>p;
    v1.push_back(p);
  }
  int left=0;
  int right=v1.size()-1;
  while(left<right){
    int temp=v1[left];
    v1[left]=v1[right];
    v1[right]=temp;
    left++;
    right--;
  }
  for(int x:v1){
    cout<<x<<" ";
  }
  return 0;
}
  
